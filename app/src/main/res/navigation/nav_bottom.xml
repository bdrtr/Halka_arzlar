        <action
            android:id="@+id/action_calculateFragment_to_commentFragment"
            app:destination="@id/fragment_comment" />
    </fragment>
    <fragment
        android:id="@+id/fragment_comment"
        android:name="com.application_3.halkaarzlar.DetailPages.commentFragment"
        android:label="fragment_comment"
        tools:layout="@layout/fragment_comment" >
